<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AsistenteEscolar.Views.AlumnosViews.AlumnoCreate">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nombre:" />
            <Entry x:Name="nombre" />
            <Label Text="Apellido:" />
            <Entry x:Name="apellido" />
            <Button Text="Guardar" x:Name="Guardar" Clicked="Guardar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>